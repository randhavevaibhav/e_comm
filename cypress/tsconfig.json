{
  "compilerOptions": {
    "strict": true,
    "forceConsistentCasingInFileNames":true,
    "target": "es5",
    "lib": ["es5", "dom"],
    "types": ["cypress", "node"],
    "esModuleInterop": true,
    "baseUrl": "..",
    "paths": {
  "@/*": ["src/*"],
  "@Cypress/*":["cypress/*"],
    }
  },
  "include": ["**/*.ts"]
}